  top:"relu36"
}
layer {
  name:"conv37_3x3_256"
  type:"Convolution"
  bottom:"relu36"
  top:"conv37"
  convolution_param {
    num_output: 256
    kernel_size: 3
    pad: 1
    weight_filler { type:"msra" }
    bias_filler { type:"constant" }
  }
}
layer {
  name:"bn37"
  type:"BatchNorm"
  bottom:"conv37"
  top:"bn37"
}
layer {
  name:"elt37"
  type:"Eltwise"
  bottom:"relu35"
  bottom:"bn37"
  top:"sum37"
  eltwise_param { operation: SUM }
}
layer {
  name:"relu37"
  type:"ReLU"
  bottom:"sum37"
  top:"relu37"
}
layer {
  name:"conv38_3x3_256"
  type:"Convolution"
  bottom:"relu37"
  top:"conv38"
  convolution_param {
    num_output: 256
    kernel_size: 3
    pad: 1
    weight_filler { type:"msra" }
    bias_filler { type:"constant" }
  }
}
layer {
  name:"bn38"
  type:"BatchNorm"
  bottom:"conv38"
  top:"bn38"
}
layer {
  name:"relu38"
  type:"ReLU"
  bottom:"bn38"
  top:"relu38"
}
layer {
  name:"conv39_3x3_256"
  type:"Convolution"
  bottom:"relu38"
